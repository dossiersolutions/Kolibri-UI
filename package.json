{
  "name": "kolibri-ui",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://dossiersolutions.github.io/kolibri-ui",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch-css": "less-watch-compiler --source-map styles public/styles --main-file main.less",
    "build-app": "react-scripts build",
    "build-lib": "babel src --out-dir dist --copy-files",
    "test": "react-scripts test --env=jsdom",
    "deploy-docs": "yarn run build-app && gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "less": "^2.7.2",
    "less-watch-compiler": "^1.8.0"
  }
}
